<form action="/createpost"method="POST">
    <div class="form-group">
        <div class="form-group">
            <label for="title">Title of post</label>
        </div>
        <div class="form-group">
            <input type="title" id="title" name="title" placeholder="Your title.."
                   value="<%= typeof title != 'undefined' ? title : '' %>">
        </div>
    </div>
    <div class="form-group">
        <div class="col-25">
            <label for="lname">Paraghrape</label>
        </div>
        <div class="form-group">
            <textarea type="para" id="para" name="para" placeholder="Your para.."
                   value="<%= typeof para != 'undefined' ? para : '' %>"></textarea>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Submit">
    </div>
</form>
</body>
</html>